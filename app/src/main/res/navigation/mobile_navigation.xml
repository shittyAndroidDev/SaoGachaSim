<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.saogachasim.ui.home.HomeFragment"
        android:label="@string/scout"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.saogachasim.ui.dashboard.DashboardFragment"
        android:label="@string/banner"
        tools:layout="@layout/fragment_dashboard" />
    <fragment
        android:id="@+id/navigation_storage"
        android:name="com.example.saogachasim.ui.storage.StorageFragment"
        android:label="@string/title_storage"
        tools:layout="@layout/storage" />

</navigation>